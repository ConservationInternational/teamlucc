citHeader("To cite teamlucc in publications use:")

citEntry(entry = "Manual",
  title        = "Tropical Ecology Assessment and Monitoring (TEAM) Network Land Use and Cover Change Toolkit: The teamlucc Package",
  author       = personList(as.person("Alex Zvoleff")),
  year         = "2014",
  url          = "http://github.com/azvoleff/teamlucc",

  textVersion  =
  paste("Alex Zvoleff (2014).",
        "Tropical Ecology Assessment and Monitoring (TEAM) Network Land Use and Cover Change Toolkit: The teamlucc Package.",
        "URL http://github.com/azvoleff/teamlucc")
)

